					}
				});

				$('#create').click(function () {
					//alert("create");
					$("#modal").iziModal('open', function (modal) {

						modal.startLoading();

						$.get('/story', function (data) {
							$("#modal .iziModal-content").html(data);

							modal.stopLoading();
						});

					});
				});

