		script(type='text/javascript').
			jQuery(document).ready(function() {
				App.init();
				PortfolioPage.init();
				var userid = $('#userid').attr('value');


				var request = $.ajax({
					url: "http://127.0.0.1:8000/api/v0/storylist/list",
					type: "GET",
					data: {id: userid},
					dataType: "json"
				});

				request.done(function (data) {
					for (var i = 0; i < Object.keys(data).length; i++) {
						$('<li class="col-md-3 col-sm-6 col-xs-12">' +
							'<a href="#" style="width: 24px;float:right;border: 0px solid;z-index:10000;" onclick=\"delete_story(\'' + data[i]['id'] + '\');return false;"><i class="fa fa-trash-o sdelete" ></i></a>' +
							'<a href="#" style="width: 24px;float:right;border: 0px solid;z-index:10000;" onclick=\"convert_story(\'' + data[i]['id'] + '\');return false;"><i class="fa fa-spinner sconvert" ></i></a>' +
							'<a href="javascript:location.href=\'/editor?id=' + data[i]['mindmap'] + '&type=mindmap\';" target="_self" style="height:70px;">' +
							'<img src="" alt="" class="img-responsive">' +
							'<div style="height:18px; background-image: url(data:image/svg+xml;base64,);"></div>' +
							'<span class="sorting-cover">' +
							'<span>' + data[i]['title'] + '</span>' +
							'<p style="height:47px;">'+data[i]['description']+'</p>'+
							//'<input type="button" value="변환하기" class="slist" style="width: 100%;" onclick="loadstory(\'' + data[i]['mindmap'] + '\')">' +
							'</span></a>' +
							'<input id="storyedit" type="button" value="편집하기" class="slist" style="width: 100%;"/>' +
							'<div style="height:20px;background-color:#fff;"></div>' +
							'</li>').appendTo("#storylist");
					}
				});

				request.fail(function (jqXHR, textStatus) {
					alert("Request failed: " + textStatus);
				});

				//$("#modal").iziModal({
				//	iframe: true,
				//	iframeHeight: 350,
				//	iframeURL: "/story"
				//});

				$("#modal").iziModal({

					title: 'Create Story',
